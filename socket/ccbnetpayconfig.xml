<?xml version="1.0" encoding="UTF-8"?>

<!-- 中国建设银行商户通知验签配置文件 -->

<ccbnetpayconfig>

<!-- 通讯端口1024~65535之间未被使用的端口 -->
  <commport>
       <value>55535</value>
   </commport>

<!-- 验签程序的线程数 -->
   <maxconn>
       <value>5</value>
   </maxconn>

<!-- 商户若有多个柜台，可按照不同的柜台号配置不同的公钥 -->
<merpos>

	<!-- 商户柜台号b2c -->
	<posid>652468641</posid>
		<!-- 对应的公钥 -->
    <pubkey></pubkey>
</merpos>

<merpos>
	<!-- 商户柜台号-手机支付 -->
	<posid>801428228</posid>
	
	<pubkey></pubkey>
</merpos>

</ccbnetpayconfig>
